<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:LiveWell;assembly=LiveWell"
             x:Class="LiveWell.GroceryLists">
  <ListView x:Name="allLists">
    <ListView.ItemTemplate>
      <DataTemplate>
        <local:CustomGroceryCell ListName="{Binding Path = ListName}" Users="{Binding Path = Users}" Products="{Binding Path = Products}"/>
      </DataTemplate>
    </ListView.ItemTemplate>
  </ListView>
</ContentPage>